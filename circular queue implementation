int enq(int q[],int r)
{
  int x;
  if( (r+1)%max=f)
  {
    printf("the queue is full");
  }
  else{
    printf("enter the element");
    scanf("%d",&x);
    r++:
    q[r]=x;
    }
    return r;
}
int deq(int [],int f,int r)
{
  int x;
  if (f==-1 && r==-1)
  {
    printf("queue is empty");
  }
  else
  {
    x=q[f];
    printf("the value deleted is",x);
    if(f==r)
    {
      f=-1;
     }
     else{
     f=(f+1)%max;}
     }
    return f;
}
void disp(int [],injt f,inht r)
{
  if(f==-1 && r==-1)
  {
    printf("queue is emnpty");
  }
  else{
    printf("%d",q[f]);
   f=(f+1)%max;
   }
   printf("%d",q[f]);
   return;
}
    
}
